{
  "manifest_version": 2,
  "name": "incepZen",
  "version": "0.1",
  	"background": {
  	"scripts": ["jquery-latest.js"]
	},
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-latest.js"]
